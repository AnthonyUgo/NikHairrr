{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,js,json,svg,png,jpg,jpeg,gif,ico,woff,woff2,ttf,eot}",
      "/assets/*"
    ]
  },
  "routes": [
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/shop",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/shop/bundles",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/shop/wigs",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/testimonials",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "mimeTypes": {
    ".json": "application/json",
    ".svg": "image/svg+xml"
  },
  "globalHeaders": {
    "cache-control": "public, max-age=31536000, immutable"
  }
}
